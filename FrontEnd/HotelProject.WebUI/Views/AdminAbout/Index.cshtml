@model List<ResultAboutDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

@*Admin About Index*@

<h1>Index</h1>
<div class="container">
    <div class="row">
        <table class="table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Title 1</th>
                    <th>Title 2</th>
                    <th>Update</th>
                   
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tbody>
                    <tr>
                        <td>@item.AboutID</td>
                        <td>@item.AboutTitle1</td>
                        <td>@item.AboutTitle2</td>                       
                        <td>                            
                            <a href="/AdminAbout/UpdateAboutData/@item.AboutID" style="margin-left:7px"><span class="text-succes"><i class="fa-solid fa-pen-to-square"></i></span></a>
                        </td>                        
                    </tr>
                </tbody>
            }
        </table>        
    </div>

</div>
