@model List<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<div class="container">
    <div class="row">
        <table class="table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Service Title</th>                  
                    <th>Edit</th>
                    
                </tr>
            </thead>
            @foreach (var item in Model)
            {
                <tbody>
                    <tr>
                        <td>@item.ServiceID</td>                      
                        <td>@item.ServiceTitle</td>
                        <td>
                            <a href="/Service/DeleteService/@item.ServiceID"><span class="text-danger"><i class="fa-solid fa-trash"></i></span></a>
                            <a href="/Service/UpdateServiceData/@item.ServiceID" style="margin-left:7px"><span class="text-succes"><i class="fa-solid fa-pen-to-square"></i></span></a>
                        </td>
                       
                    </tr>
                </tbody>
            }
        </table>
        <a class="btn btn-dark my-2" href="/Service/AddService/">Add Service</a>
    </div>

</div>

